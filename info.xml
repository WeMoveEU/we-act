<?xml version="1.0"?>
<extension key="eu.wemove.we-act" type="module">
  <file>we_act</file>
  <name>WeAct</name>
  <description>A CiviCRM extension that provides entry points to process and store in CiviCRM actions taken by WeMove members from external tools</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Romain Thouvenin</author>
    <email>romain@wemove.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/WeMoveEU/we-act</url>
    <url desc="Documentation">https://github.com/WeMoveEU/we-act</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-04-22</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/WeAct</namespace>
  </civix>
</extension>
